<!--pages/addFault/addFault.wxml-->
<view class="all">
  <form class="faultForm" bindsubmit="formSubmit">
    <view class="name_part">
      <view class="name_txt">申报人</view>
      <input class="u_name" disabled name="f_u_name" value="{{u_name}}"></input>
    </view>
    <view class="id_part">
      <view class="id_txt">申报人账号</view>
      <input class="u_id" disabled name="f_u_Id" value="{{u_Id}}"></input>
    </view>
    <view class="content_part">
      <view class="content_txt">申报内容</view>
      <textarea class="u_content" name="f_content" placeholder="请在此输入..." placeholder-style="font-size:30rpx;"></textarea> 
    </view>
    <button form-type="submit" class="btn">提交</button>
  </form>
</view>
